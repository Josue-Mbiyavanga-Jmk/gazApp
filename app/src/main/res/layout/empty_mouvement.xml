<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:id="@+id/portion_name_etablissement">

    <RelativeLayout
        android:layout_centerInParent="true"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/contenaireTageu"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        >

        <ImageView

            style="@style/img_lab"
            app:srcCompat="@drawable/ic_fuel_truck"
            android:id="@+id/i1"
            android:tint="@color/md_material_blue_600"
            android:layout_height="120dp"
            android:layout_width="120dp"
            android:layout_centerHorizontal="true"
            />


        <TextView
            android:id="@+id/helptext"
            style="@style/txt_help_menu"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/i1"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="20dp"
            android:text="Il n'y a aucun mouvement d'entrée dans le dépôt enregistré pour l'instant."
            android:textColor="@color/NewGridLoginBckg"
            android:textSize="10sp"
            android:textAlignment="center"
            android:textStyle="normal" />











    </RelativeLayout>




</RelativeLayout>